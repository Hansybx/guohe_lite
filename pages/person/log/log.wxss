/* pages/core/person/log/log.wxss */
.logo {
  width: 150rpx;
  height: 150rpx;
  animation: myfirst 5s;
  margin: 40rpx auto;
  display: block;
  animation-iteration-count: 100;
  animation-timing-function: linear;
  animation-duration: 1.8s;
}

.top {
  margin: 25rpx;
  margin-bottom: 100rpx;
  font-size: 30rpx;
  text-align: center;
  color: #778899;
}

.text {
  margin: 25rpx;
  margin-top: 5rpx;
  margin-bottom: 55rpx;
  font-size: 30rpx;
  text-align: left;
  color: #778899;
}

.text_title {
  margin: 30rpx;
  margin-bottom: 10rpx;
  background-color: #fff;
  font-size: 35rpx;
  text-align: left;
}

page {
  background-color: #fff;
  background-position: center;
  background-size: 100%;
  text-align: center;
}